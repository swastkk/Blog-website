<%- include("partials/header") -%>
<body>
  <div class="container">
    <h1>Home</h1>
    <p><%= startingH %></p>
    <% posts.forEach(function(post){ %>
    <h1><%= post.title %></h1>
    <p>
      <%= post.content.substring(0, 300) + "..." %>
      <a href="/posts/<%= post.title %>"> Read More... </a>
    </p>
    <% }) %>
  </div>
</body>

